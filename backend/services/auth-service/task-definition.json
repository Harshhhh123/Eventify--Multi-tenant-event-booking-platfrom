{
    
    "containerDefinitions": [
        {
            "name": "auth-service",
            "image": "623753037953.dkr.ecr.ap-south-1.amazonaws.com/eventify-auth-service:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "auth-service-4000-tcp",
                    "containerPort": 4000,
                    "hostPort": 4000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "JWT_SECRET",
                    "value": "eventify_dev_jwt_secret_9283749827349827"
                },
                {
                    "name": "AWS_REGION",
                    "value": "ap-south-1"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/eventify-auth",
                    "awslogs-create-group": "true",
                    "awslogs-region": "ap-south-1",
                    "awslogs-stream-prefix": "auth"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "family": "eventify-auth-task",
    "taskRoleArn": "arn:aws:iam::623753037953:role/eventify-auth-task-role",
    "executionRoleArn": "arn:aws:iam::623753037953:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
 
    "volumes": [],
 
  
    "placementConstraints": [],
   
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
 
    "enableFaultInjection": false,
    "tags": []
}
